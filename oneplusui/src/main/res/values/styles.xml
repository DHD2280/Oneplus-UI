<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="OplusTextAppearance" parent="@android:style/TextAppearance">
        <item name="android:textStyle">normal</item>
        <item name="android:textAllCaps">false</item>
        <item name="android:fontFamily">sans-serif</item>
    </style>

    <style name="OplusTextAppearance.Button" parent="@style/OplusTextAppearance">
        <item name="android:textSize">16sp</item>
        <item name="android:lineSpacingMultiplier">1.2</item>
        <item name="fontFamily">sans-serif-medium</item>
    </style>

    <style name="OplusTextAppearanceHeadline6" parent="@style/OplusTextAppearance">
        <item name="android:textSize">16sp</item>
        <item name="android:lineSpacingMultiplier">1.05</item>
        <item name="android:fontFamily">sans-serif-medium</item>
        <item name="android:textColor">@color/text_color_primary</item>
    </style>

    <style name="OplusTextAppearanceArticleBody" parent="@style/OplusTextAppearance">
        <item name="android:textSize">14sp</item>
        <item name="android:lineSpacingMultiplier">1.1</item>
    </style>

    <style name="OplusTextAppearanceBody" parent="@style/OplusTextAppearance">
        <item name="android:textSize">14sp</item>
        <item name="android:lineSpacingMultiplier">1.1</item>
    </style>

    <style name="OplusInputTextAppearance" parent="@style/OplusTextAppearanceHeadline6">
        <item name="android:fontFamily">sans-serif-regular</item>
    </style>

    <style name="OplusTextAppearanceCaption" parent="@style/OplusTextAppearance">
        <item name="android:textSize">10sp</item>
        <item name="android:lineSpacingMultiplier">@dimen/oplus_spacing_multiplier_cap</item>
    </style>

    <style name="OplusInputTitleStyle" parent="@style/OplusTextAppearanceHeadline6">
        <item name="android:textColor">@color/oplus_input_title_color</item>
        <item name="android:minHeight">@dimen/oplus_inputView_title_minheight</item>
    </style>

    <style name="PreferenceTitleStyle" parent="@style/OplusTextAppearance">
        <item name="android:textSize">16sp</item>
        <item name="android:lineSpacingMultiplier">1.05</item>
        <item name="android:fontFamily">sans-serif-medium</item>
        <item name="android:textColor">@color/text_color_primary</item>
        <item name="android:textAlignment">viewStart</item>
    </style>

    <style name="PreferenceSummaryStyle" parent="@style/OplusTextAppearanceBody">
        <item name="android:typeface">normal</item>
        <item name="android:textColor">@color/text_color_secondary</item>
        <item name="android:textAlignment">viewStart</item>
    </style>

    <style name="PreferenceIcon" parent="">
        <item name="android:layout_marginTop">@dimen/preference_icon_margin_top</item>
        <item name="android:layout_marginBottom">@dimen/preference_icon_margin_bottom</item>
        <item name="android:layout_marginEnd">@dimen/preference_icon_margin_right</item>
    </style>

    <style name="PreferenceContent" parent="">
        <item name="android:paddingTop">@dimen/preference_text_content_padding_top</item>
        <item name="android:paddingBottom">@dimen/preference_text_content_padding_bottom</item>
        <item name="android:layout_marginStart">@dimen/preference_text_margin_left</item>
    </style>

    <style name="PreferenceStatusText" parent="">
        <item name="android:textAppearance">@style/OplusTextAppearanceBody</item>
        <item name="android:textColor">@color/text_color_secondary</item>
    </style>

    <style name="PreferenceStatusText.Tiny" parent="@style/PreferenceStatusText">
        <item name="android:textSize">14sp</item>
    </style>

    <style name="OplusPreferenceCategoryStyle" parent="@android:style/Widget">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:minHeight">@dimen/preference_category_text_height</item>
        <!--        <item name="android:layout_marginTop">@dimen/preference_item_margin_top</item>-->
        <!--        <item name="android:layout_marginBottom">@dimen/preference_item_margin_bottom</item>-->
        <item name="android:layout_marginLeft">@dimen/preference_item_padding_side</item>
        <item name="android:layout_marginRight">@dimen/preference_item_padding_side</item>
        <item name="android:paddingStart">@dimen/preference_category_padding_start</item>
        <item name="android:textSize">14sp</item>
        <item name="android:lineSpacingMultiplier">1</item>
        <item name="android:fontFamily">sans-serif-medium</item>
        <item name="android:textColor">@color/preference_category_text</item>
        <item name="android:textAlignment">viewStart</item>
    </style>

    <style name="AppTheme.Dialog" parent="ThemeOverlay.Material3.MaterialAlertDialog">
        <item name="android:dialogCornerRadius">32dp</item>
        <item name="android:background">@color/oplusColorSurfaces</item>
        <item name="android:textColor">@color/text_color_primary</item>
        <item name="android:backgroundDimAmount">0.40</item>
        <item name="materialAlertDialogTitleTextStyle">@style/MaterialAlertDialog.Title.Text</item>
        <item name="materialAlertDialogBodyTextStyle">@style/MaterialAlertDialog.Body.Text</item>
        <item name="android:animation">@style/Animation.Oplus.Dialog</item>
    </style>

    <style name="Animation.Oplus.Dialog" parent="@android:style/Animation">
        <item name="android:windowEnterAnimation">@anim/oplus_bottom_dialog_enter</item>
        <item name="android:windowExitAnimation">@anim/oplus_bottom_dialog_exit</item>
    </style>

    <style name="MaterialAlertDialog.Title.Text" parent="MaterialAlertDialog.MaterialComponents.Title.Text">
        <item name="android:textColor">@color/text_color_primary</item>
        <item name="android:textSize">20sp</item>
        <item name="android:fontFamily">sans-serif-medium</item>
    </style>

    <style name="MaterialAlertDialog.Body.Text" parent="MaterialAlertDialog.MaterialComponents.Body.Text">
        <item name="android:fontFamily">sans-serif</item>
        <item name="android:textColor">@color/oplusColorPrimaryNeutral</item>
        <item name="android:textSize">14sp</item>
        <item name="android:lineSpacingMultiplier">1.1</item>
    </style>

    <style name="ThemeOverlay.App.BottomSheetDialog" parent="ThemeOverlay.Material3.BottomSheetDialog">
        <item name="bottomSheetStyle">@style/ModalBottomSheetDialog</item>
    </style>

    <style name="ModalBottomSheetDialog" parent="Widget.Material3.BottomSheet.Modal">
        <item name="android:windowBackground">@android:color/transparent</item>
        <item name="android:windowIsFloating">false</item>
        <item name="android:background">@drawable/bottom_sheet_background</item>
        <item name="android:navigationBarColor">@color/oplusColorSurfaces</item>
        <item name="android:windowElevation">17dp</item>
        <item name="panelBackground">@drawable/panel_bg_without_shadow</item>
        <item name="android:windowAnimationStyle">@style/Animation.Design.BottomSheetDialog</item>
        <item name="backgroundTint">@color/oplusColorSurfaces</item>
    </style>

    <style name="Widget.App.Material3.Switch" parent="Widget.Material3.CompoundButton.MaterialSwitch">
        <item name="android:scaleX">0.8</item>
        <item name="android:scaleY">0.8</item>
        <item name="android:thumb">@drawable/switch_thumb</item>
        <item name="track">@drawable/switch_track</item>
        <item name="thumbTint">@color/white</item>
        <item name="trackDecorationTint">@color/transparent</item>
        <item name="trackTint">@drawable/switch_track_selector</item>
    </style>

    <style name="Widget.App.AppCompat.Switch" parent="Widget.AppCompat.CompoundButton.Switch">
        <item name="track">@drawable/switch_track_selector</item>
        <item name="trackTint">@drawable/switch_track_selector</item>
        <item name="android:thumb">@drawable/switch_thumb</item>
    </style>

    <style name="Widget.App.PopupMenu" parent="Widget.Material3.PopupMenu">
        <item name="textAppearanceLargePopupMenu">@style/TextAppearance.App.BodyLarge</item>
        <item name="textAppearanceSmallPopupMenu">@style/TextAppearance.App.BodySmall</item>
        <item name="popupMenuBackground">@drawable/custom_popup_background</item>
        <item name="android:textColor">@color/text_color_primary</item>
        <item name="divider">@color/colorDivider</item>
        <item name="android:dividerHeight">@dimen/list_divider_height</item>
    </style>

    <style name="TextAppearance.App.BodyLarge" parent="TextAppearance.Material3.BodyLarge">
        <item name="android:textColor">@color/text_color_primary</item>
    </style>

    <style name="TextAppearance.App.BodySmall" parent="TextAppearance.Material3.BodySmall">
        <item name="android:textColor">@color/text_color_primary</item>
    </style>

    <style name="Widget.CompoundButton.RadioButton" parent="@style/Widget.AppCompat.CompoundButton.RadioButton">
        <item name="android:button">@drawable/custom_radio_selector</item>
    </style>

    <style name="Widget.App.Snackbar" parent="Widget.Material3.Snackbar">
        <item name="backgroundTint">@color/oplusColorSurfaces</item>
    </style>

    <style name="Widget.App.Snackbar.TextView" parent="Widget.MaterialComponents.Snackbar.TextView">
        <item name="android:textColor">@color/text_color_primary</item>
        <item name="android:textStyle">normal</item>
        <item name="android:textAllCaps">false</item>
        <item name="android:fontFamily">sans-serif</item>
        <item name="android:lineSpacingMultiplier">1.05</item>
    </style>

    <style name="Widget.App.Slider" parent="Widget.Material3.Slider">
        <item name="labelStyle">@style/Widget.App.Tooltip</item>
    </style>

    <!-- Material ToolTip -->
    <style name="Widget.App.Tooltip" parent="Widget.Material3.Tooltip">
        <item name="android:padding">10dp</item>
        <item name="android:textColor">@color/text_color_primary</item>
        <item name="backgroundTint">@color/oplusColorSurfaces</item>
    </style>

    <!-- Oplus ToolTip -->
    <style name="Widget.Oplus.Tooltip" parent="Widget.Material3.Tooltip">
        <item name="android:textAppearance">@style/OplusTextAppearanceBody</item>
        <item name="android:padding">10dp</item>
        <item name="android:textColor">@color/text_color_primary</item>
        <item name="backgroundTint">@color/oplusColorSurfaces</item>
        <item name="shapeAppearance">@style/ShapeAppearance.Material3.Tooltip</item>
    </style>

    <!-- AppBarLayout -->
    <style name="Widget.App.AppBarLayout" parent="Widget.Material3.AppBarLayout">
        <item name="background">@drawable/top_appbar_bg</item>
    </style>

    <!-- Toolbar -->
    <style name="Widget.App.Toolbar" parent="Widget.Material3.Toolbar">
        <item name="colorOnSurface">@color/text_color_primary</item>
        <item name="titleTextAppearance">@style/TextAppearance.App.TitleLarge</item>
        <item name="subtitleTextAppearance">@style/TextAppearance.App.TitleMedium</item>
        <item name="navigationIconTint">@color/text_color_primary</item>
    </style>

    <style name="TextAppearance.App.TitleLarge" parent="TextAppearance.Material3.TitleLarge">
        <item name="android:textColor">@color/text_color_primary</item>
    </style>

    <style name="TextAppearance.App.TitleMedium" parent="TextAppearance.Material3.TitleMedium">
        <item name="android:textColor">@color/text_color_secondary</item>
    </style>

    <style name="Widget.BottomNavigationView" parent="Base.Widget.Material3.BottomNavigationView">
        <item name="android:background">@color/bottomNavigationBackground</item>
        <item name="itemRippleColor">@color/transparent</item>
        <item name="itemActiveIndicatorStyle">@style/Widget.BottomNavigationView.ActiveIndicator
        </item>
        <item name="itemIconTint">@color/text_color_primary</item>
        <item name="itemTextColor">@color/text_color_primary</item>
    </style>

    <style name="Widget.BottomNavigationView.ActiveIndicator" parent="">
        <item name="android:color">@color/transparent</item>
    </style>

    <style name="EditTextPreferenceStyle" parent="Preference.DialogPreference.EditTextPreference">
        <item name="iconSpaceReserved">false</item>
        <item name="dialogLayout">@layout/oplus_preference_dialog_edittext</item>
    </style>

    <style name="Preferences.Oplus.Category" parent="">
        <item name="android:layout">@layout/oplus_preference_category</item>
    </style>

    <style name="Preferences.Oplus.Preference" parent="">
        <item name="android:layout">@layout/oplus_preference</item>
    </style>

    <style name="Preferences.Oplus.Preference.List" parent="">
        <item name="android:layout">@layout/oplus_preference_list</item>
    </style>

    <style name="Preferences.OplusMenuPreference" parent="@style/Preferences.Oplus.Preference.List">
        <item name="android:popupMenuStyle">
            @style/Widget.Preference.SimpleMenuPreference.PopupMenu
        </item>
        <item name="android:popupTheme">
            @style/ThemeOverlay.Preference.SimpleMenuPreference.PopupMenu
        </item>
    </style>

    <style name="Preferences.Oplus.Preference.Switch" parent="@style/Preferences.Oplus.Preference">
        <item name="android:widgetLayout">@layout/oplus_preference_widget_switch</item>
    </style>

    <style name="Preferences.Oplus.Preference.Jump" parent="Preferences.Oplus.Preference">
        <item name="android:widgetLayout">@layout/oplus_preference_widget_jump</item>
    </style>

    <style name="Preferences.Oplus.Preference.Slider" parent="@style/Preferences.Oplus.Preference">
        <item name="android:layout">@layout/oplus_preference_slider</item>
    </style>
    
    <style name="Preferences.Oplus.Preference.Seekbar" parent="@style/Preferences.Oplus.Preference">
        <item name="android:layout">@layout/oplus_preference_seekbar</item>
    </style>

    <style name="Preferences.Oplus.Preference.SectionSeekbar" parent="@style/Preferences.Oplus.Preference">
        <item name="android:layout">@layout/oplus_preference_sectionseekbar</item>
    </style>

    <style name="DefaultDialogItemTextStyle" parent="@style/TextAppearance.AppCompat.Medium">
        <item name="android:textSize">16sp</item>
        <item name="android:textColor">@color/oplus_dialog_button_text_color_center</item>
        <item name="fontFamily">sans-serif-medium</item>
    </style>

    <style name="DialogAnimation" parent="android:Animation.Dialog">
        <item name="android:windowEnterAnimation">@anim/oplus_bottom_dialog_enter</item>
        <item name="android:windowExitAnimation">@anim/oplus_bottom_dialog_exit</item>
    </style>

    <style name="Widget.Preference.SimpleMenuPreference.PopupMenu" parent="android:Widget">
        <item name="android:popupBackground">@drawable/custom_popup_background</item>

        <item name="listMarginHorizontal">@dimen/simple_menu_margin</item>
        <item name="listMarginVertical">8dp</item>
        <item name="listItemPadding">@dimen/preference_card_margin_horizontal</item>
        <item name="listElevation">8dp</item>

        <item name="dialogMarginHorizontal">12dp</item>
        <item name="dialogMarginVertical">24dp</item>
        <item name="dialogItemPadding">24dp</item>
        <item name="dialogElevation">24dp</item>
        <item name="dialogMaxWidth">@dimen/simple_menu_dialog_max_width</item>

        <item name="unit">@dimen/simple_menu_unit</item>
        <item name="maxUnits">@integer/simple_menu_max_units</item>
    </style>

    <style name="ThemeOverlay.Preference.SimpleMenuPreference.PopupMenu" parent="android:ThemeOverlay">
        <item name="popupBackgroundRadius">50dp</item>
    </style>

    <style name="Animation.Preference.SimpleMenuCenter" parent="android:Animation">
        <item name="android:windowEnterAnimation">@anim/oplus_popup_list_enter</item>
        <item name="android:windowExitAnimation">@anim/oplus_popup_list_exit</item>
    </style>

    <style name="Widget.Oplus.SeekBar" parent="">
        <item name="oplusSeekBarAdaptiveVibrator">true</item>
        <item name="oplusSeekBarBackgroundColor">@color/oplus_seekbar_background_selector</item>
        <item name="oplusSeekBarBackgroundRadius">@dimen/oplus_seekbar_background_radius</item>
        <item name="oplusSeekBarMaxWidth">@dimen/oplus_seekbar_view_max_width</item>
        <item name="oplusSeekBarMinHeight">36dp</item>
        <item name="oplusSeekBarPhysicsEnable">true</item>
        <item name="oplusSeekBarProgressColor">@color/oplus_seekbar_progress_selector</item>
        <item name="oplusSeekBarProgressFull">false</item>
        <item name="oplusSeekBarProgressPaddingHorizontal">@dimen/oplus_seekbar_progress_padding_horizontal</item>
        <item name="oplusSeekBarProgressRadius">@dimen/oplus_seekbar_progress_radius</item>
        <item name="oplusSeekBarProgressScaleRadius">@dimen/oplus_seekbar_progress_scale_radius</item>
        <item name="oplusSeekBarShowProgress">true</item>
        <item name="oplusSeekBarShowThumb">true</item>
        <item name="oplusSeekBarStartMiddle">false</item>
        <item name="oplusSeekBarThumbColor">@color/oplus_seekbar_progress_selector</item>
        <item name="oplusSeekBarThumbOutRadius">@dimen/oplus_seekbar_thumb_out_radius</item>
        <item name="oplusSeekBarThumbShadowColor">@color/oplus_seekbar_thumb_shadow_color</item>
        <item name="oplusLabelStyle">@style/Widget.Oplus.Tooltip</item>
    </style>

    <style name="Widget.Oplus.Slider" parent="">
        <item name="oplusSeekBarAdaptiveVibrator">true</item>
        <item name="oplusSeekBarBackgroundColor">@color/oplus_seekbar_background_selector</item>
        <item name="oplusSeekBarBackgroundRadius">@dimen/oplus_seekbar_background_radius</item>
        <item name="oplusSeekBarMaxWidth">@dimen/oplus_seekbar_view_max_width</item>
        <item name="oplusSeekBarMinHeight">36dp</item>
        <item name="oplusSeekBarPhysicsEnable">true</item>
        <item name="oplusSeekBarProgressColor">@color/oplus_seekbar_progress_selector</item>
        <item name="oplusSeekBarProgressFull">false</item>
        <item name="oplusSeekBarProgressPaddingHorizontal">@dimen/oplus_seekbar_progress_padding_horizontal</item>
        <item name="oplusSeekBarProgressRadius">@dimen/oplus_seekbar_progress_radius</item>
        <item name="oplusSeekBarProgressScaleRadius">@dimen/oplus_seekbar_progress_scale_radius</item>
        <item name="oplusSeekBarThumbColor">@color/oplus_seekbar_progress_selector</item>
        <item name="oplusSeekBarThumbOutRadius">@dimen/oplus_seekbar_thumb_out_radius</item>
        <item name="oplusSeekBarThumbShadowColor">@color/oplus_seekbar_thumb_shadow_color</item>
        <item name="oplusLabelStyle">@style/Widget.Oplus.Tooltip</item>
    </style>

    <style name="Widget.Oplus.SectionSeekBar" parent="@style/Widget.Oplus.Slider">
        <item name="oplusSeekBarPhysicsEnable">false</item>
        <item name="oplusSeekBarProgressPaddingHorizontal">@dimen/oplus_section_seekbar_progress_padding_horizontal</item>
    </style>

    <style name="Widget.OplusComponents.Tooltip" parent="Widget.MaterialComponents.Tooltip">
        <item name="android:textAppearance">@style/TextAppearance.OplusComponents.Tooltip</item>
        <item name="shapeAppearance">@style/ShapeAppearance.MaterialComponents.Tooltip</item>
    </style>

    <style name="TextAppearance.OplusComponents.Tooltip" parent="TextAppearance.MaterialComponents.Tooltip">
        <item name="android:textColor">@color/text_color_primary</item>
    </style>

    <!-- Edit Text -->
    <style name="Widget.Oplus.EditText" parent="@android:style/Widget.EditText">
        <item name="android:textAppearance">@style/OplusInputTextAppearance</item>
        <item name="android:textColorHint">@color/oplus_color_label_tertiary</item>
        <item name="android:paddingTop">9dp</item>
        <item name="android:paddingBottom">9dp</item>
        <item name="android:minHeight">@dimen/oplus_edit_text_height_min</item>
        <item name="android:editTextColor">@color/oplus_edit_text_color</item>
        <item name="android:textAlignment">viewStart</item>
        <item name="collapsedTextColor">@color/oplus_color_secondary_neutral</item>
        <item name="defaultStrokeColor">@color/colorDivider</item>
        <item name="disabledStrokeColor">@color/oplus_textinput_stroke_color_disabled</item>
        <item name="editTextDeleteIconNormal">@drawable/ic_edit_text_delete</item>
        <item name="editTextDeleteIconPressed">@drawable/ic_edit_text_delete</item>
        <item name="focusStrokeWidth">@dimen/oplus_textinput_focus_stroke_width</item>
        <item name="strokeColor">@android:color/system_accent1_400</item>
        <item name="strokeWidth">@dimen/oplus_textinput_stroke_width</item>
    </style>


    <style name="Widget.Oplus.EditText.HintAnim" parent="@style/Widget.Oplus.EditText">
        <item name="collapsedTextSize">@dimen/oplus_textinput_hint_text_size</item>
        <item name="cornerRadius">@dimen/oplus_textinput_corner_radius</item>
        <item name="focusStrokeWidth">@dimen/oplus_textinput_focus_stroke_width</item>
        <item name="hintEnabled">true</item>
        <item name="strokeWidth">@dimen/oplus_textinput_stroke_width</item>
    </style>

    <style name="Widget.Oplus.EditText.HintAnim.Line" parent="@style/Widget.Oplus.EditText.HintAnim">
        <item name="android:paddingLeft">0dp</item>
        <item name="android:paddingTop">27dp</item>
        <item name="android:paddingRight">0dp</item>
        <item name="android:paddingBottom">15dp</item>
        <item name="backgroundMode">line</item>
    </style>

    <style name="Widget.Oplus.EditText.HintAnim.Rectangle" parent="@style/Widget.Oplus.EditText.HintAnim">
        <item name="android:textStyle">bold</item>
        <item name="android:paddingLeft">16dp</item>
        <item name="android:paddingRight">10dp</item>
        <item name="android:paddingBottom">12dp</item>
        <item name="backgroundMode">rectangle</item>
        <item name="rectModePaddingTop">12dp</item>
    </style>

    <style name="Widget.Oplus.Input" parent="@android:style/Widget.EditText">
        <item name="android:textAppearance">@style/OplusTextAppearance</item>
        <item name="android:paddingTop">@dimen/oplus_input_edit_text_no_title_padding_top</item>
        <item name="android:paddingBottom">@dimen/oplus_input_edit_text_no_title_padding_bottom</item>
        <item name="android:minHeight">0dp</item>
        <item name="android:maxLines">1</item>
        <item name="android:singleLine">true</item>
        <item name="android:editTextColor">@color/oplus_edit_text_color</item>
        <item name="android:textAlignment">viewStart</item>
        <item name="collapsedTextColor">@color/oplus_color_secondary_neutral</item>
        <item name="backgroundMode">line</item>
        <item name="defaultStrokeColor">@color/colorDivider</item>
        <item name="disabledStrokeColor">@color/oplus_textinput_stroke_color_disabled</item>
        <item name="editTextDeleteIconNormal">@drawable/ic_edit_text_delete</item>
        <item name="editTextDeleteIconPressed">@drawable/ic_edit_text_delete</item>
        <item name="focusStrokeWidth">@dimen/oplus_textinput_focus_stroke_width</item>
        <item name="hintEnabled">false</item>
        <item name="strokeColor">@android:color/system_accent1_400</item>
        <item name="strokeWidth">@dimen/oplus_input_preference_stroke_width</item>
        <item name="quickDelete">true</item>
    </style>

    <style name="Animation.Slide" parent="@android:style/Animation.Activity">
        <item name="android:taskOpenEnterAnimation">@anim/oplus_open_slide_enter</item>
        <item name="android:taskOpenExitAnimation">@anim/oplus_open_slide_exit</item>
        <item name="android:taskCloseEnterAnimation">@anim/oplus_close_slide_enter</item>
        <item name="android:taskCloseExitAnimation">@anim/oplus_close_slide_exit</item>
        <item name="android:taskToFrontEnterAnimation">@anim/oplus_open_slide_enter</item>
        <item name="android:taskToFrontExitAnimation">@anim/oplus_open_slide_exit</item>
        <item name="android:taskToBackEnterAnimation">@anim/oplus_close_slide_enter</item>
        <item name="android:taskToBackExitAnimation">@anim/oplus_close_slide_exit</item>
    </style>

    <style name="Animation.Slide.Settings" parent="@style/Animation.Slide">
        <item name="android:activityOpenEnterAnimation">@anim/oplus_open_slide_enter</item>
        <item name="android:activityOpenExitAnimation">@anim/oplus_open_slide_exit</item>
        <item name="android:activityCloseEnterAnimation">@anim/oplus_close_slide_enter</item>
        <item name="android:activityCloseExitAnimation">@anim/oplus_close_slide_exit</item>
    </style>

</resources>